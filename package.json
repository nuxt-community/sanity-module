{
  "name": "@nuxtjs/sanity",
  "version": "0.3.2",
  "description": "Sanity integration for Nuxt.js",
  "keywords": [
    "nuxt",
    "module",
    "nuxt-module",
    "sanity",
    "sanity-io",
    "cms"
  ],
  "repository": "nuxt-community/sanity-module",
  "license": "MIT",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "siroc build",
    "dev": "nuxt-ts example",
    "lint": "eslint --fix --ext .js,.ts,.vue .",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test",
    "release": "yarn build && yarn test && release-it",
    "test": "yarn lint && yarn build && jest --runInBand"
  },
  "dependencies": {
    "defu": "^3.1.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "latest",
    "@babel/preset-env": "latest",
    "@babel/preset-typescript": "latest",
    "@nuxt/types": "latest",
    "@nuxt/typescript-build": "latest",
    "@nuxt/typescript-runtime": "latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@nuxtjs/module-test-utils": "2.0.0-2",
    "@nuxtjs/tailwindcss": "^3.0.0",
    "@release-it/conventional-changelog": "^1.1.4",
    "@sanity/client": "^1.149.18",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "latest",
    "@types/webpack-env": "^1.15.2",
    "@vue/test-utils": "^1.0.3",
    "babel-jest": "26.3.0",
    "eslint": "latest",
    "jest": "26.4.1",
    "jest-serializer-vue-tjw": "^3.14.0",
    "lint-staged": "^10.2.11",
    "nuxt-edge": "latest",
    "puppeteer": "^5.2.1",
    "release-it": "^13.6.6",
    "siroc": "latest",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "chalk": "^4.1.0",
    "consola": "^2.14.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}
