{
  "name": "@nuxtjs/sanity",
  "version": "0.3.9",
  "description": "Sanity integration for Nuxt.js",
  "keywords": [
    "nuxt",
    "module",
    "nuxt-module",
    "sanity",
    "sanity-io",
    "cms"
  ],
  "repository": "nuxt-community/sanity-module",
  "license": "MIT",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "vetur",
    "templates"
  ],
  "scripts": {
    "build": "siroc build",
    "dev": "nuxt-ts example",
    "lint": "eslint --fix --ext .js,.ts,.vue .",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test",
    "release": "yarn build && yarn test && release-it",
    "test": "yarn lint && yarn build && jest --runInBand"
  },
  "husky": {
    "gitHooks": {
      "pre-commit": "lint-staged"
    }
  },
  "resolutions": {
    "rollup-plugin-esbuild": "2.3.0"
  },
  "dependencies": {
    "defu": "3.1.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-typescript": "7.12.1",
    "@nuxt/types": "2.14.7",
    "@nuxt/typescript-build": "2.0.3",
    "@nuxt/typescript-runtime": "2.0.0",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@nuxtjs/module-test-utils": "2.0.0-3",
    "@nuxtjs/tailwindcss": "3.1.0",
    "@release-it/conventional-changelog": "2.0.0",
    "@sanity/client": "2.0.1",
    "@types/fs-extra": "9.0.2",
    "@types/jest": "26.0.14",
    "@types/webpack-env": "1.15.3",
    "@vue/test-utils": "1.1.0",
    "babel-jest": "26.5.2",
    "eslint": "7.11.0",
    "husky": "4.3.0",
    "jest": "26.5.3",
    "jest-serializer-vue-tjw": "3.15.0",
    "lint-staged": "10.4.0",
    "nuxt-edge": "latest",
    "playwright": "1.5.1",
    "release-it": "14.1.0",
    "siroc": "0.4.0"
  },
  "peerDependencies": {
    "chalk": "^3.0.0",
    "consola": "^2.15.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "vetur": {
    "tags": "vetur/tags.json",
    "attributes": "vetur/attributes.json"
  },
  "volta": {
    "node": "10.22.1"
  }
}
