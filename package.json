{
  "name": "@nuxtjs/sanity",
  "version": "0.7.1",
  "description": "Sanity integration for Nuxt.js",
  "keywords": [
    "nuxt",
    "module",
    "nuxt-module",
    "sanity",
    "sanity-io",
    "cms"
  ],
  "repository": "nuxt-community/sanity-module",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.es.js"
    },
    "./": "./",
    "./sanity-content": "./dist/components/sanity-content.js",
    "./sanity-image": "./dist/components/sanity-image.js"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "vetur",
    "templates"
  ],
  "scripts": {
    "build": "siroc build",
    "dev": "nuxt-ts example",
    "lint": "eslint --fix --ext .js,.ts,.vue .",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test",
    "test": "yarn lint && jest"
  },
  "husky": {
    "gitHooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "defu": "3.2.2",
    "upath": "2.0.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "7.13.10",
    "@babel/preset-env": "7.13.10",
    "@babel/preset-typescript": "7.13.0",
    "@nuxt/test-utils": "0.1.3",
    "@nuxt/types": "2.15.2",
    "@nuxt/typescript-build": "2.0.6",
    "@nuxt/typescript-runtime": "2.0.2",
    "@nuxtjs/eslint-config-typescript": "6.0.0",
    "@nuxtjs/tailwindcss": "3.4.2",
    "@release-it/conventional-changelog": "2.0.1",
    "@sanity/client": "2.2.6",
    "@types/fs-extra": "9.0.8",
    "@types/jest": "26.0.20",
    "@types/webpack-env": "1.16.0",
    "@vue/test-utils": "1.1.3",
    "babel-jest": "26.6.3",
    "eslint": "7.21.0",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "jest-serializer-vue-tjw": "3.16.0",
    "lint-staged": "10.5.4",
    "nuxt": "2.15.2",
    "playwright": "1.9.1",
    "release-it": "14.4.1",
    "siroc": "0.7.0"
  },
  "peerDependencies": {
    "chalk": "^3.0.0",
    "consola": "^2.15.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "vetur": {
    "tags": "vetur/tags.json",
    "attributes": "vetur/attributes.json"
  },
  "volta": {
    "node": "12.20.1"
  }
}
